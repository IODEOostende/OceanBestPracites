{
  "name": "ocean-best-practices",
  "version": "0.0.1",
  "license": "AGPL-3.0-only",
  "repository": "github:iodepo/OceanBestPractices",
  "bin": {
    "ocean-best-practices": "bin/app.js"
  },
  "scripts": {
    "build": "./bin/build.sh",
    "cdk:list": "cdk list",
    "cdk:dev:deploy": "cdk deploy Dev/* && npm run cdk:dev:update-config",
    "cdk:dev:destroy": "cdk destroy Dev/*",
    "cdk:dev:diff": "cdk diff Dev/*",
    "cdk:dev:update-config": "ts-node bin/update-config.ts dev",
    "cdk:prod:deploy": "cdk deploy Prod/* && npm run cdk:prod:update-config",
    "cdk:prod:diff": "cdk diff Prod/*",
    "cdk:prod:update-config": "ts-node bin/update-config.ts prod",
    "cdk:staging:deploy": "cdk deploy Staging/* && npm run cdk:staging:update-config",
    "cdk:staging:diff": "cdk diff Staging/*",
    "cdk:staging:update-config": "ts-node bin/update-config.ts staging",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "npm run lint:tsc && npm run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:tsc": "tsc --project tsconfig-lint-cdk.json"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.129.0",
    "@aws-cdk/aws-certificatemanager": "^1.129.0",
    "@aws-cdk/aws-cloudfront": "^1.129.0",
    "@aws-cdk/aws-cloudfront-origins": "^1.129.0",
    "@aws-cdk/aws-codepipeline-actions": "^1.129.0",
    "@aws-cdk/aws-ec2": "^1.129.0",
    "@aws-cdk/aws-elasticsearch": "^1.129.0",
    "@aws-cdk/aws-events": "^1.129.0",
    "@aws-cdk/aws-events-targets": "^1.129.0",
    "@aws-cdk/aws-iam": "^1.129.0",
    "@aws-cdk/aws-lambda": "^1.129.0",
    "@aws-cdk/aws-s3": "^1.129.0",
    "@aws-cdk/aws-s3-deployment": "^1.129.0",
    "@aws-cdk/aws-s3-notifications": "^1.129.0",
    "@aws-cdk/aws-secretsmanager": "^1.129.0",
    "@aws-cdk/aws-sns": "^1.129.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.129.0",
    "@aws-cdk/aws-ssm": "^1.129.0",
    "@aws-cdk/core": "^1.129.0",
    "@aws-cdk/pipelines": "^1.129.0",
    "@aws-sdk/client-cloudformation": "^3.37.0",
    "@aws-sdk/client-s3": "^3.37.0",
    "@aws-sdk/client-ssm": "^3.37.0",
    "source-map-support": "^0.5.16",
    "type-fest": "^2.5.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.175",
    "@types/node": "^12.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "aws-cdk": "^1.129.0",
    "bufferutil": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-import-newlines": "^1.1.5",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-unicorn": "^37.0.1",
    "jest": "26.6.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.4.3",
    "utf-8-validate": "^5.0.2"
  }
}
